class A {
	function constructor(mult=1) {
		this.mult = mult;
		this.value = 0;
	}

	function add(amount) {
		this.value = this.value + amount * this.mult;
	}
}

obj = new A();
print(obj.value);
obj.add(10);
print(obj.value);

if(obj.value == 11) {
	print("false");
}
else if (obj.value > 5){
	print("true");
}
else {
	print("false");
}

