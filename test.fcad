function sum(...args) {
	result = 0;
	print(args);
	for(n in args) {
		result = result + n;
	}
	return result;
}

print(sum(1, 2, 3, 4));
print(sum(...arr));


new_arr = [x * 2 for x in arr];
print(new_arr);
print(sum(...new_arr));